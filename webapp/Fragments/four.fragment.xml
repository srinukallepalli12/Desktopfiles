	<Dialog class="noDataForm sapUiSizeCompact" title="{i18n>StudentDetails}" contentHeight="80%">
		<SearchField liveChange="searchfield4"/>
		<List id="listitems4" items="{/}" mode="MultiSelect"   headerText="ShipperName">
			<StandardListItem title="{ShipperName}"></StandardListItem>
		</List>
		<buttons>
